class Solution {
public:
    int search(vector<int>& nums, int target) {
    int i=0;
    int j=nums.size()-1;
   
    while((i<j)){
        if((target!=nums[i])and(target!=nums[j])){
            i++;
            j--;
        }
    else if(target==nums[i])return i;
    else return j;
    } 
        if(i<nums.size()){
        if(nums[i]==target)return i;}
    return -1;
    
    
    }
};
