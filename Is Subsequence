class Solution {
public:
    bool isSubsequence(string s, string t) {
        int val=0;
        if(s.length()==0){return true;}
        for(int i=0;i<t.length();i++){
            if(s[val]==t[i]){
                val++;
            }
        if(val==s.length())return true;
        }

    return false;
    }
};
