class Solution {
public:
    bool isAnagram(string s, string t) {
        map<char,int>hashing1;
        map<char,int>hashing2;
        for(int i=0;i<s.length();i++){
            hashing1[s[i]]++;
        }
        for(int i=0;i<t.length();i++){
            hashing2[t[i]]++;
        }
    return hashing1==hashing2;
    }
};
